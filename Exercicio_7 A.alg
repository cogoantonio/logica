inicio
    inteiro x
    inteiro y
    inteiro z
    escrever "informe x"
    ler x
    escrever "informe y"
    ler y
    escrever "informe z"
    ler z
    se ( ( z >= x ) e ( z <= y ) ) entao
        escrever "Esta dentro do intervalo"
    senao
        escrever "Esta fora do intervalo"
    fimse
fim

